#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/macluiggy/Documents/PROGRAMMING/TYPESCRIPT/microsoft-typescript/node_modules/.pnpm/ts-node@10.4.0_@types+node@16.11.6/node_modules/ts-node/dist/node_modules:/home/macluiggy/Documents/PROGRAMMING/TYPESCRIPT/microsoft-typescript/node_modules/.pnpm/ts-node@10.4.0_@types+node@16.11.6/node_modules/ts-node/node_modules:/home/macluiggy/Documents/PROGRAMMING/TYPESCRIPT/microsoft-typescript/node_modules/.pnpm/ts-node@10.4.0_@types+node@16.11.6/node_modules:/home/macluiggy/Documents/PROGRAMMING/TYPESCRIPT/microsoft-typescript/node_modules/.pnpm/node_modules:/home/macluiggy/Documents/PROGRAMMING/TYPESCRIPT/microsoft-typescript/node_modules:/home/macluiggy/Documents/PROGRAMMING/TYPESCRIPT/node_modules:/home/macluiggy/Documents/PROGRAMMING/node_modules:/home/macluiggy/Documents/node_modules:/home/macluiggy/node_modules:/home/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/macluiggy/Documents/PROGRAMMING/TYPESCRIPT/microsoft-typescript/node_modules/.pnpm/ts-node@10.4.0_@types+node@16.11.6/node_modules/ts-node/dist/node_modules:/home/macluiggy/Documents/PROGRAMMING/TYPESCRIPT/microsoft-typescript/node_modules/.pnpm/ts-node@10.4.0_@types+node@16.11.6/node_modules/ts-node/node_modules:/home/macluiggy/Documents/PROGRAMMING/TYPESCRIPT/microsoft-typescript/node_modules/.pnpm/ts-node@10.4.0_@types+node@16.11.6/node_modules:/home/macluiggy/Documents/PROGRAMMING/TYPESCRIPT/microsoft-typescript/node_modules/.pnpm/node_modules:/home/macluiggy/Documents/PROGRAMMING/TYPESCRIPT/microsoft-typescript/node_modules:/home/macluiggy/Documents/PROGRAMMING/TYPESCRIPT/node_modules:/home/macluiggy/Documents/PROGRAMMING/node_modules:/home/macluiggy/Documents/node_modules:/home/macluiggy/node_modules:/home/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/bin-transpile.js" "$@"
else
  exec node  "$basedir/../../dist/bin-transpile.js" "$@"
fi
